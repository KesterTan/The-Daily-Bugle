<%-  include("partials/header") -%>

<div class="publish-body">
    <h1 class="publish">Publish</h1>
    <form action="/create" method="post">
      <div class="title">
        <label for="user_title">Title:</label>
        <input type="text" id="user_title" name="composeTitle">
      </div>
      <div class="content">
        <label for="user_content">Content:</label>
        <textarea type="text" id="user_content" name="composeContent" rows="6"></textarea>
      </div>
      <div class="submit">
        <button type="submit" name ="submit" autocomplete="off">Publish</button>
      </div>
    </form>
</div>

<%- include("partials/footer") -%> 

